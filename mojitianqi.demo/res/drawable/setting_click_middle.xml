<?xml version="1.0" encoding="utf-8"?>
<selector android:constantSize="true"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <gradient android:startColor="@color/setting_pressed_start" android:endColor="@color/setting_pressed_end" android:angle="270.0" />
            <stroke android:width="1.0px" android:color="@color/setting_pressed_end" />
        </shape>
    </item>
    <item android:drawable="@drawable/setting_list_middle" />
</selector>
