    <item android:left="@dimen/picker_view_bg_topitem_pading_left" android:top="@dimen/picker_view_bg_topitem_pading_top" android:right="@dimen/picker_view_bg_topitem_pading_right" android:bottom="@dimen/picker_view_bg_topitem_pading_bottom">
