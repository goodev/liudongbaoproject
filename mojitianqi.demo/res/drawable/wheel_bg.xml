        <shape android:shape="rectangle">
            <gradient android:startColor="@color/picker_view_bg_bottom_startcolor" android:endColor="@color/picker_view_bg_bottom_endcolor" android:angle="90.0" android:centerColor="@color/picker_view_bg_bottom_centercolor" />
